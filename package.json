{
  "name": "euro-2021",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14",
    "npm": ">=7"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@sentry/react": "^6.2.0",
    "@sentry/tracing": "^6.2.0",
    "date-fns": "^2.17.0",
    "firebase": "^8.2.7",
    "node-sass": "^4.14.1",
    "notistack": "^1.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^2.2.7",
    "react-number-format": "^4.4.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "reactfire": "3.0.0-rc.0",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "start": "npm i && react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src --ext js,jsx",
    "lint:functions": "cd functions && npm run lint",
    "prettier:check": "prettier --check \"{src,functions}/**/*.{js,jsx,ts,md}\"",
    "prettier:write": "prettier --write \"{src,functions}/**/*.{js,jsx,ts,md}\"",
    "test:rules": "firebase emulators:exec --only firestore \"react-scripts test --testPathPattern 'firebase/rules/__tests__/.+\\.spec\\.js' --env=node --runInBand\"",
    "test": "react-scripts test --testPathIgnorePatterns='<rootDir>/src/firebase/rules/__tests__'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^1.2.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "babel-plugin-transform-imports": "^2.0.0",
    "firebase-admin": "^9.5.0",
    "prettier": "^2.2.1",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.2.2"
  }
}
